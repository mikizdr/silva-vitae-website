{
  "name": "silva-vitae-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc -b",
    "preview": "vite preview",
    "lint": "pnpm exec eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "pnpm exec prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "format:check": "pnpm exec prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^9.0.0",
    "globals": "^13.24.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^5.4.12"
  }
}
